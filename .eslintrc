{
  "extends": [
    "plugin:prettier/recommended",
    "react-app",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "eslint/interface-name-prefix": "on",
    "eslint/explicit-function-return-type": "on",
    "eslint/explicit-module-boundary-types": "on",
    "eslint/no-explicit-any": "on"
  }
}
